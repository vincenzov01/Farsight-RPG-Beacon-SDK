{
  "name": "farsigt-rpg-beacon-sdk",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "sandbox-server": "vite --port 7620 --mode staging",
    "sandbox": "run-s build-scss sandbox-server",
    "build": "run-s build-scss && vite build",
    "build-scss": "sass ./src/rollTemplates/host.scss ./public/host.css --no-source-map --no-charset",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.3",
    "@roll20-official/beacon-sdk": "^0.0.13",
    "handlebars": "^4.7.8",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "primevue": "^4.3.7",
    "uuid": "^9.0.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.3.7",
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25"
  }
}
